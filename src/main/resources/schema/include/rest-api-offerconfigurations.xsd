        <xs:annotation>
            <xs:documentation>
                Specifies a [unitsMin, unitsMax) range - including unitsMin and excluding unitsMax - for which the given
                charge is applied.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="unitsMin" type="xs:long">
                <xs:annotation>
                    <xs:documentation>
                        Lower limit of the tiering level.
                        It must be zero for the first tiering level.
                        It must be equal to the upper limit (unitsMax) of the previous tiering level, except the first
                        tiering level.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="unitsMax" type="xs:long">
                <xs:annotation>
                    <xs:documentation>
                        Upper limit for the tiering level. Blank value means there is no limit.
                        It must be blank for the last tiering level.
                        It must be equal to the lower limit (unitsMin) of the next tiering level, except the
                        last tiering level.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="charge" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        Charge concerning the tiering level.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
