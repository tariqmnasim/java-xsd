            <xs:element minOccurs="0" name="units" type="xs:decimal"/>
            <xs:element minOccurs="0" name="measuringUnitKey" type="xs:string"/>
            <xs:element name="netAmount" type="xs:decimal"/>
            <xs:element name="grossAmount" type="xs:decimal"/>
