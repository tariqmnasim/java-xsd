<?xml version="1.0" encoding="UTF-8"?>
<xs:schema elementFormDefault="qualified" jaxb:version="2.1" targetNamespace="http://www.travellazy.com/product/model"
           xmlns:tl="http://www.travellazy.com/product/model" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
           xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:annotation>
        <xs:documentation>
            This is the model used for all customer based
            information and
            manipulation operations.
        </xs:documentation>
    </xs:annotation>
    <xs:include schemaLocation="rest-api-commons.xsd"/>
    <xs:element name="address" type="tl:Address"/>
    <xs:complexType name="Address">
        <xs:complexContent>
            <xs:extension base="tl:AbstractAddress">
                <xs:sequence>
                    <xs:element minOccurs="0" name="addressLine1" type="xs:string"/>
                    <xs:element minOccurs="0" name="addressLine2" type="xs:string"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="createAddress" type="tl:CreateAddress"/>
    <xs:complexType name="CreateAddress">
        <xs:complexContent>
            <xs:extension base="tl:AbstractAddress"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="updateAddress" type="tl:UpdateAddress"/>
    <xs:complexType name="UpdateAddress">
        <xs:complexContent>
            <xs:extension base="tl:AbstractAddress"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType abstract="true" name="AbstractInvoiceDetails">
        <xs:sequence>
            <xs:element minOccurs="0" name="address" type="tl:CreateAddress"/>
            <xs:element minOccurs="0" name="invoiceItemizationTypeKey" type="xs:string"/>
            <xs:element minOccurs="0" name="taxModeKey" type="xs:string"/>
            <xs:element default="1" name="invoiceInterval" type="xs:int"/>
            <xs:element minOccurs="0" name="invoiceDeliveryMethodKey" type="xs:string"/>
            <xs:element minOccurs="0" name="nextInvoicePeriod" type="tl:InvoicePeriod"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="TaxExemption">
        <xs:sequence>
            <xs:element minOccurs="0" name="taxExemptionReasonKey" type="xs:string"/>
            <xs:element minOccurs="0" name="taxExemptionCode" type="xs:string"/>
            <xs:element name="validFrom" type="xs:dateTime"/>
            <xs:element minOccurs="0" name="validTo" type="xs:dateTime"/>
        </xs:sequence>
        <xs:attribute default="false" name="isTaxExempt" type="xs:boolean"/>
    </xs:complexType>
    <xs:element name="createInvoiceDetails" type="tl:CreateInvoiceDetails"/>
    <xs:complexType name="CreateInvoiceDetails">
        <xs:complexContent>
            <xs:extension base="tl:AbstractInvoiceDetails">
                <xs:sequence>
                    <xs:element minOccurs="0" name="billCycleId" type="xs:long"/>
                    <xs:element minOccurs="0" name="taxExemption" type="tl:TaxExemption"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="invoiceCurrency" type="tl:isoCurrency">
                        <xs:annotation>
                            <xs:documentation>
                                Three letter iso-code of the customers currency. Charges of different currency are
                                exchanged at billrun, so that the customer receives the invoice in this currency. Cannot
                                be changed.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="updateInvoiceDetails" type="tl:UpdateInvoiceDetails"/>
    <xs:complexType name="UpdateInvoiceDetails">
        <xs:complexContent>
            <xs:extension base="tl:AbstractInvoiceDetails">
                <xs:sequence>
                    <xs:element minOccurs="0" name="futureBillCycleId" type="xs:long"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="invoiceDetails" type="tl:InvoiceDetails"/>
    <xs:complexType name="InvoiceDetails">
        <xs:complexContent>
            <xs:extension base="tl:AbstractInvoiceDetails">
                <xs:sequence>
                    <xs:element minOccurs="0" name="futureBillCycleId" type="xs:long"/>
                    <xs:element name="currentBillCycleId" type="xs:long"/>
                    <xs:element minOccurs="0" name="lastInvoicedPeriod" type="tl:InvoicePeriod"/>
                    <xs:element name="taxExemption" type="tl:TaxExemption"/>
                    <xs:element minOccurs="0" name="pendingTaxExemption" type="tl:TaxExemption"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="invoiceCurrency" type="tl:isoCurrency">
                        <xs:annotation>
                            <xs:documentation>
                                Three letter iso-code of the customers currency. Charges of different currency are
                                exchanged at billrun, so that the customer receives the invoice in this currency. Cannot
                                be changed.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="Parameters">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="parameter" type="tl:Parameter"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Parameter">
        <xs:sequence>
            <xs:element name="key" type="xs:string"/>
            <xs:element minOccurs="0" name="value" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AdditionalDetails">
        <xs:annotation>
            <xs:documentation>
                A list of additional customer details.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="detail" type="tl:AdditionalDetailsEntry"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AdditionalDetailsEntry">
        <xs:annotation>
            <xs:documentation>
                Represents one additional customer detail entry.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="key" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The key (name) for the additional customer detail entry.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="value" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The value for the additional customer detail entry.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="DocumentIdentifiers">
        <xs:annotation>
            <xs:documentation>
                A list of identifiers of documents to be linked to customer. Identifiers are be obtained from the
                /documents resource, after the documents are successfully uploaded to the document repository.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="documentIdentifier" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        An identifier of a document to be linked to customer. Identifiers are be obtained from the
                        /documents resource, after the documents are successfully uploaded to the document repository.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="Priority">
        <xs:restriction base="xs:integer">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="9"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType abstract="true" name="CreateOrder">
        <xs:sequence>
            <xs:element minOccurs="0" name="priority" type="tl:Priority"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType abstract="true" name="AbstractCreateCustomer">
        <xs:complexContent>
            <xs:extension base="tl:CreateOrder">
                <xs:sequence>
                    <xs:choice>
                        <xs:element name="channelId" type="xs:long"/>
                        <xs:element name="channelKey" type="xs:string"/>
                    </xs:choice>
                    <xs:element minOccurs="0" name="operator" type="xs:string"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="customerId" type="xs:long"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="parentAccountNumber" type="xs:long"/>
                    <xs:element minOccurs="0" name="legacyAccountNumber" type="xs:string"/>
                    <xs:element minOccurs="0" name="contactDetails" type="tl:CreateContactDetails"/>
                    <xs:element minOccurs="0" name="invoiceDetails" type="tl:CreateInvoiceDetails"/>
                    <xs:element minOccurs="0" name="paymentDetails" type="tl:CreatePaymentDetails"/>
                    <xs:element minOccurs="0" name="creditCheckDetails" type="tl:CreateCreditCheckDetails"/>
                    <xs:element minOccurs="0" name="createUser" type="tl:CreateUser"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="offerIndependentFeatures" type="tl:CreateFeatures"/>
                    <xs:element maxOccurs="1" minOccurs="0" name="customizedOffers" type="tl:CreateCustomizedOffers"/>
                    <xs:element minOccurs="0" name="notes" type="xs:string"/>
                    <xs:element minOccurs="0" name="parameters" type="tl:Parameters"/>
                    <xs:element minOccurs="0" name="documentIdentifiers" type="tl:DocumentIdentifiers"/>
                    <xs:element minOccurs="0" name="additionalDetails" type="tl:AdditionalDetails"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="createResidentialCustomer" type="tl:CreateResidentialCustomer"/>
    <xs:complexType name="CreateResidentialCustomer">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCreateCustomer">
                <xs:sequence>
                    <xs:element name="residentialCustomerAttributes" type="tl:ResidentialCustomerAttributes"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="prospectResidentialCustomer" type="tl:ProspectResidentialCustomer"/>
    <xs:complexType name="ProspectResidentialCustomer">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCreateCustomer">
                <xs:sequence>
                    <xs:element minOccurs="0" name="residentialCustomerAttributes"
                                type="tl:ResidentialCustomerAttributes"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="createBusinessCustomer" type="tl:CreateBusinessCustomer"/>
    <xs:complexType name="CreateBusinessCustomer">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCreateCustomer">
                <xs:sequence>
                    <xs:element name="businessCustomerAttributes" type="tl:BusinessCustomerAttributes"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="prospectBusinessCustomer" type="tl:ProspectBusinessCustomer"/>
    <xs:complexType name="ProspectBusinessCustomer">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCreateCustomer">
                <xs:sequence>
                    <xs:element minOccurs="0" name="businessCustomerAttributes" type="tl:BusinessCustomerAttributes"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="createTenant" type="tl:CreateTenant"/>
    <xs:complexType name="CreateTenant">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCreateCustomer">
                <xs:sequence>
                    <xs:element name="tenantAttributes" type="tl:TenantAttributes"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="createCustomer" type="tl:CreateCustomer"/>
    <xs:complexType name="CreateCustomer">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCreateCustomer">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="1" name="customerTypeKey" type="xs:string"/>
                    <xs:element minOccurs="0" name="customerAttributes" type="tl:CustomerAttributes"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="createProspect" type="tl:CreateProspect"/>
    <xs:complexType name="CreateProspect">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCreateCustomer">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="1" name="customerTypeKey" type="xs:string"/>
                    <xs:element minOccurs="0" name="customerAttributes" type="tl:CustomerAttributes"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="prospectResponse" type="tl:ProspectResponse"/>
    <xs:complexType name="ProspectResponse">
        <xs:sequence>
            <xs:element name="prospectId" type="xs:long"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType abstract="true" name="AbstractCreditCheckDetails">
        <xs:sequence>
            <xs:element minOccurs="0" name="identificationTypeKey" type="xs:string"/>
            <xs:element minOccurs="0" name="identificationNumber" type="xs:string"/>
            <xs:element minOccurs="0" name="residentialStatusKey" type="xs:string"/>
            <xs:element minOccurs="0" name="residentialStatusKeyPreviousAddress" type="xs:string"/>
            <xs:element minOccurs="0" name="gender" type="tl:GenderType"/>
            <xs:element minOccurs="0" name="countryOfResidence" type="xs:string"/>
            <xs:element minOccurs="0" name="employmentStatusKey" type="xs:string"/>
            <xs:element minOccurs="0" name="employerName" type="xs:string"/>
            <xs:element minOccurs="0" name="agreesToCreditCheck" type="xs:boolean"/>
            <xs:element minOccurs="0" name="timeAtCurrentAddressKey" type="xs:string"/>
            <xs:element minOccurs="0" name="timeAtPreviousAddressKey" type="xs:string"/>
            <xs:element minOccurs="0" name="creditCardNumbersKey" type="xs:string"/>
            <xs:element minOccurs="0" name="timeInCurrentJobKey" type="xs:string"/>
            <xs:element minOccurs="0" name="initialCreditDecisionType" type="tl:CreditDecisionType"/>
            <xs:element minOccurs="0" name="riskGrade" type="xs:int"/>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="GenderType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="FEMALE"/>
            <xs:enumeration value="MALE"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="CreditDecisionType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="ACCEPTED"/>
            <xs:enumeration value="REFERRED"/>
            <xs:enumeration value="DECLINED"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:element name="createCreditCheckDetails" type="tl:CreateCreditCheckDetails"/>
    <xs:complexType name="CreateCreditCheckDetails">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCreditCheckDetails"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType abstract="true" name="AbstractCreateUser">
        <xs:sequence>
            <xs:element name="userName" type="xs:string"/>
            <xs:element name="firstName" type="xs:string"/>
            <xs:element name="lastName" type="xs:string"/>
            <xs:element name="email" type="xs:string"/>
            <xs:element name="password" type="xs:string"/>
            <xs:element minOccurs="0" name="passwordExpirationDate" type="xs:dateTime"/>
            <xs:element name="role" type="xs:string"/>
            <xs:element name="languageKey" type="xs:string"/>
            <xs:element minOccurs="0" name="securityQuestion" type="xs:string"/>
            <xs:element minOccurs="0" name="securityAnswer" type="xs:string"/>
        </xs:sequence>
        <xs:attribute name="isActive" type="xs:boolean"/>
    </xs:complexType>
    <xs:element name="createUser" type="tl:CreateUser"/>
    <xs:complexType name="CreateUser">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCreateUser"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:simpleType name="StatusType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="NEW"/>
            <xs:enumeration value="ACTIVE"/>
            <xs:enumeration value="PENDING_DISCONNECT"/>
            <xs:enumeration value="DISCONNECTED"/>
            <xs:enumeration value="CLOSED"/>
            <xs:enumeration value="PENDING_ACTIVE"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:element name="customerSearchResults" type="tl:CustomerSearchResults"/>
    <xs:complexType name="CustomerSearchResults">
        <xs:complexContent>
            <xs:extension base="tl:AbstractPagedList">
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" minOccurs="0" name="customerSearchResult"
                                type="tl:CustomerSearchResult"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="customerSearchResult" type="tl:CustomerSearchResult"/>
    <xs:complexType name="CustomerSearchResult">
        <xs:sequence>
            <xs:element minOccurs="0" name="customerId" type="xs:long"/>
            <xs:element minOccurs="0" name="customerTitleKey" type="xs:string"/>
            <xs:element minOccurs="0" name="firstName" type="xs:string"/>
            <xs:element minOccurs="0" name="lastName" type="xs:string"/>
            <xs:element minOccurs="0" name="middleName" type="xs:string"/>
            <xs:element minOccurs="0" name="businessName" type="xs:string"/>
            <xs:element minOccurs="0" name="accountStatusKey" type="tl:StatusType"/>
            <xs:element minOccurs="0" name="address" type="tl:Address"/>
            <xs:element maxOccurs="1" minOccurs="0" name="customizedOffers" type="tl:CustomizedOfferSearchResults"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CustomizedOfferSearchResults">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="customizedOffer" type="tl:CustomizedOfferSearchResult"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CustomizedOfferSearchResult">
        <xs:sequence>
            <xs:element minOccurs="0" name="name" type="xs:string"/>
            <xs:element minOccurs="0" name="key" type="xs:string"/>
            <xs:element minOccurs="0" name="serviceIdentifier" type="xs:string"/>
            <xs:element minOccurs="0" name="address" type="tl:Address"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="searchResults" type="tl:SearchResults"/>
    <xs:complexType name="SearchResults">
        <xs:complexContent>
            <xs:extension base="tl:AbstractPagedList">
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" minOccurs="0" name="searchResult" type="tl:SearchResult"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SearchResult">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" name="field" type="tl:Field"/>
        </xs:sequence>
        <xs:attribute name="score" type="xs:float"/>
    </xs:complexType>
    <xs:complexType name="Field">
        <xs:sequence>
            <xs:element name="key" type="xs:string"/>
            <xs:element maxOccurs="unbounded" name="value" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="customizedOffer" type="tl:AbstractCustomizedOffer"/>
    <xs:complexType abstract="true" name="AbstractCustomizedOffer">
        <xs:sequence>
            <xs:element name="id" type="xs:long"/>
            <xs:element name="offerKey" type="xs:string"/>
            <xs:element name="internalName" type="xs:string"/>
            <xs:element name="invoiceName" type="xs:string"/>
            <xs:element name="offerTypeKey" type="xs:string"/>
            <xs:element name="offerStatusKey" type="xs:string"/>
            <xs:element minOccurs="0" name="activation" type="xs:dateTime"/>
            <xs:element minOccurs="0" name="termination" type="xs:dateTime"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CustomizedBaseOffers">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="CustomizedBaseOffer"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="customizedBaseOffer" type="tl:CustomizedBaseOffer"/>
    <xs:complexType name="CustomizedBaseOffer">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCustomizedOffer">
                <xs:sequence>
                    <xs:element minOccurs="0" name="parentOfferId" type="xs:long"/>
                    <xs:element minOccurs="0" name="serviceAccountId" type="xs:long"/>
                    <xs:element minOccurs="0" name="serviceIdentifier" type="xs:string"/>
                    <xs:element minOccurs="0" name="serviceStatusKey" type="tl:StatusType"/>
                    <xs:element minOccurs="0" name="serviceBarLevel" type="xs:int"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="customizedBundleOffer" type="tl:CustomizedBundleOffer"/>
    <xs:complexType name="CustomizedBundleOffer">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCustomizedOffer">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="customizedBaseOffers" type="tl:CustomizedBaseOffers"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="customizedOffers" type="tl:CustomizedOffers"/>
    <xs:complexType name="CustomizedOffers">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="customizedOffer" type="tl:AbstractCustomizedOffer"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CreateCustomizedOfferType">
        <xs:complexContent>
            <xs:extension base="tl:CreateOrder">
                <xs:sequence>
                    <xs:choice maxOccurs="unbounded" minOccurs="1">
                        <xs:element name="offerConfiguration" type="tl:RetrieveOfferConfiguration"/>
                        <xs:element name="baseOffer" type="tl:CreateCustomizedBaseOffer"/>
                        <xs:element name="bundleOffer" type="tl:CreateCustomizedBundleOffer"/>
                    </xs:choice>
                    <xs:element minOccurs="0" name="activationDate" type="xs:dateTime"/>
                    <xs:choice minOccurs="1">
                        <xs:element name="channelId" type="xs:long"/>
                        <xs:element name="channelKey" type="xs:string"/>
                    </xs:choice>
                    <xs:element minOccurs="0" name="operator" type="xs:string"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="CreateCustomizedOffer" type="tl:CreateCustomizedOfferType"/>
    <xs:element name="createCustomizedOffers" type="tl:CreateCustomizedOffers"/>
    <xs:complexType name="CreateCustomizedOffers">
        <xs:choice maxOccurs="unbounded" minOccurs="1">
            <xs:element name="customizedOffer" type="tl:AbstractCreateCustomizedOffer"/>
            <xs:element name="offerConfiguration" type="tl:RetrieveOfferConfiguration"/>
            <xs:element name="baseOffer" type="tl:CreateCustomizedBaseOffer"/>
            <xs:element name="bundleOffer" type="tl:CreateCustomizedBundleOffer"/>
        </xs:choice>
    </xs:complexType>
    <xs:complexType abstract="true" name="AbstractCreateCustomizedOffer">
        <xs:sequence>
            <xs:element name="offerKey" type="xs:string"/>
            <xs:element maxOccurs="1" minOccurs="0" name="offerFeatures" type="tl:CreateFeatures"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RetrieveOfferConfiguration">
        <xs:complexContent>
            <xs:restriction base="tl:CreateCustomizedBaseOffer">
                <xs:sequence>
                    <xs:element name="offerKey" type="xs:string"/>
                </xs:sequence>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="CreateCustomizedBaseOffers">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="baseOffer" type="tl:CreateCustomizedBaseOffer"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CreateCustomizedBaseOffer">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCreateCustomizedOffer">
                <xs:sequence>
                    <xs:choice minOccurs="0">
                        <xs:element name="services" type="tl:CreateServices"/>
                        <xs:element name="serviceFeatures" type="tl:CreateFeatures"/>
                    </xs:choice>
                    <xs:element maxOccurs="1" minOccurs="0" name="offerGroupKey" type="xs:string"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="CreateCustomizedBundleOffer">
        <xs:complexContent>
            <xs:extension base="tl:AbstractCreateCustomizedOffer">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="baseOffers" type="tl:CreateCustomizedBaseOffers"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="UpgradeBehavior">
        <xs:all>
            <xs:element name="isUpgrade" type="xs:boolean"/>
            <xs:element minOccurs="0" name="earlyTerminationFeeHandlingTypeKey" type="xs:string"/>
            <xs:element minOccurs="0" name="earlyTerminationFeeHandlingAmount" type="xs:decimal"/>
            <xs:element minOccurs="0" name="upDowngradeFeeFeatureKey" type="xs:string"/>
        </xs:all>
    </xs:complexType>
    <xs:complexType abstract="true" name="AbstractChangeCustomizedOffer">
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="offerFeatures" type="tl:AbstractFeatures"/>
        </xs:sequence>
