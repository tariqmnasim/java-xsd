    </xs:complexType>
    <xs:complexType name="CustOfferUsage">
        <xs:annotation>
            <xs:documentation>
                Represents the sum of all detailed usage records for one bill type.
                That could be the sum of all &#8216;International Calls&#8217; for a mobile offer, where &#8216;International
                Calls&#8217; would be the bill type.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:ChargeBase">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="1" name="billTypeKey" type="xs:string"/>
                    <xs:element maxOccurs="1" minOccurs="1" name="units" type="xs:long"/>
                    <xs:element maxOccurs="1" minOccurs="1" name="measuringUnitKey" type="xs:string"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="CustOfferUsageList">
        <xs:annotation>
            <xs:documentation>
                A list of all bill types, along with their usage sums.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="usageSumPerBillType" type="tl:CustOfferUsage"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="CustomizedOffer">
        <xs:annotation>
            <xs:documentation>
                Charges and usage sums for a customized offer, along with all service identifiers and net and gross sums
                of all expenses.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="1" name="id" type="xs:long"/>
            <xs:element maxOccurs="1" minOccurs="1" name="offerInvoiceText" type="xs:string"/>
            <xs:element maxOccurs="1" minOccurs="1" name="netAmount" type="xs:decimal"/>
            <xs:element maxOccurs="1" minOccurs="1" name="grossAmount" type="xs:decimal"/>
            <xs:element maxOccurs="1" minOccurs="1" name="currency" type="tl:isoCurrency">
                <xs:annotation>
                    <xs:documentation>
                        The currency of the related customized offer's currency.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="serviceIdentifiers" type="tl:ServiceIdentifiersList"/>
            <xs:element maxOccurs="1" minOccurs="0" name="charges" type="tl:CustOfferChargeList"/>
            <xs:element maxOccurs="1" minOccurs="0" name="usageSumsPerBillType" type="tl:CustOfferUsageList"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ServiceIdentifiersList">
        <xs:annotation>
            <xs:documentation>
                A list of all service identifiers, gathered from all charges and usages. Each service identifier occurs
                only once.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="id" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="transactions" type="tl:Transactions"/>
    <xs:complexType name="Transactions">
        <xs:annotation>
            <xs:documentation>
                The list of financial transactions including all invoices, payments for one customer.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="transaction" type="tl:Transaction"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="Transaction">
        <xs:annotation>
            <xs:documentation>
                One particular transaction, typeKey gives the type of the transaction like INVOICE, PAYMENT.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="date" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>
                        The effective date of the transaction.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="typeKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The transaction type indicates the type of the line.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="paymentChannelTypeKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The channel type in case of a payment. Values are from ino_ar_payment_mgmt.pmttrxchannels
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="referenceId" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The reference number for the transaction, e.g. invoice id, payment id.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="releatedTo" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The ID to which the transaction is related, e.g. in case of a floating adjustment
                        this contains the invoice id where the floating adjustment appears on; if it is
                        an invoice the field is empty.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="sourceCustomerId" type="xs:long">
                <xs:annotation>
                    <xs:documentation>
                        In case of transfer to another customer this is the customer id where the amount was transferred
                        to.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="amount" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The amount of the transaction e.g. the total amount of an invoice or a payment.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="effectiveAmount" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The effective amount of the transaction e.g. the amount on an invoice minus resolved disputes in
                        favour of the customer.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="rollingBalance" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The rolling balance summing up all transactions ordered by date.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="disputedAmount" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        Disputes of an item on an invoice.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="undisputedItemBalance" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The outstanding amount of an invoice, if a dispute is accepted. This will be the amount of
                        the invoice minus the payments already assigned to this invoice minus the accepted disputed
                        amount.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="currency" type="tl:isoCurrency">
                <xs:annotation>
                    <xs:documentation>
                        The currency for this transaction's balances.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="createBalance" type="tl:CreateBalance"/>
    <xs:complexType name="CreateBalance">
        <xs:annotation>
            <xs:documentation>
                The balance to be created.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="balanceType" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The balance type, for which to create a balance for.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="value" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The initial value of the created balance.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="activation" type="xs:date">
                <xs:annotation>
                    <xs:documentation>
                        The date on which this balance should be activated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="termination" type="xs:date">
                <xs:annotation>
                    <xs:documentation>
                        The date on which this balance should be terminated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="referenceKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The external reference key.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="balances" type="tl:BalanceList"/>
    <xs:complexType name="BalanceList">
        <xs:annotation>
            <xs:documentation>
                A list with all found balances.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:AbstractPagedList">
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" minOccurs="0" name="balance" type="tl:Balance"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="balance" type="tl:Balance"/>
    <xs:complexType name="Balance">
        <xs:complexContent>
            <xs:extension base="tl:AbstractBalance"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType abstract="true" name="AbstractBalance">
        <xs:annotation>
            <xs:documentation>
                One particular balance
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="id" type="tl:nonNegativeLong">
                <xs:annotation>
                    <xs:documentation>
                        The id representing this balance.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="type" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The type key of this balance.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="value" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The value of the balance.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="unit" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The unit key of this balance.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="activation" type="xs:date">
                <xs:annotation>
                    <xs:documentation>
                        The date on which this balance got activated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="termination" type="xs:date">
                <xs:annotation>
                    <xs:documentation>
                        The date on which this balance got terminated.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="referenceKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The external reference key.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="balanceTransaction" type="tl:BalanceTransaction"/>
    <xs:complexType name="BalanceTransaction">
        <xs:sequence>
            <xs:element name="id" type="tl:nonNegativeLong">
                <xs:annotation>
                    <xs:documentation>
                        The id of the history entry
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="balanceId" type="tl:nonNegativeLong">
                <xs:annotation>
                    <xs:documentation>
                        The id of the balance, where this entry belongs to.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="value" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The value of this balance after the delta value got applied.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="deltaValue" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The value which increases or decreases the current balance value.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="referenceId" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Id of the trigger which caused the balance modification.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="referenceType" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Type of the trigger which caused the balance modification.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="originatingBalanceId" type="tl:nonNegativeLong">
                <xs:annotation>
                    <xs:documentation>
                        The id of the originating balance.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="balanceDetails" type="tl:BalanceDetails"/>
    <xs:complexType name="BalanceDetails">
        <xs:annotation>
            <xs:documentation>
                One particular balance and its history entries.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:AbstractBalance">
                <xs:sequence>
                    <xs:element name="transactions" type="tl:BalanceTransactionList"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="balanceTransactions" type="tl:BalanceTransactionList"/>
    <xs:complexType name="BalanceTransactionList">
        <xs:annotation>
            <xs:documentation>
                A list with all balance history entries.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:AbstractPagedList">
                <xs:sequence>
                    <xs:element maxOccurs="unbounded" minOccurs="0" name="transaction" type="tl:BalanceTransaction"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="balanceDelta" type="tl:BalanceDelta"/>
    <xs:complexType name="BalanceDelta">
        <xs:annotation>
            <xs:documentation>
                The request object to update a specific balance.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="value" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The delta value which should be applied to the current balance.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="referenceId" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Id of the trigger which caused the balance modification.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="referenceType" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Type of the trigger which caused the balance modification.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="originatingBalanceId" type="tl:nonNegativeLong">
                <xs:annotation>
                    <xs:documentation>
                        The id of the originating balance.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="FeatureGroupCategories">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="featureGroupCategory"
                        type="tl:FeatureGroupDisplayCategory"/>
        </xs:sequence>
    </xs:complexType>
</xs:schema>