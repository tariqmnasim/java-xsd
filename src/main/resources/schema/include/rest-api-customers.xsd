    <xs:element name="updateCustomizedOffer" type="tl:UpdateCustomizedOffer"/>
    <xs:complexType name="UpdateCustomizedOffer">
        <xs:complexContent>
            <xs:extension base="tl:CreateOrder">
                <xs:sequence>
                    <xs:element name="action" type="tl:OfferAction"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="OfferAction">
        <xs:sequence>
            <xs:element minOccurs="0" name="effectiveDate" type="tl:dateWithOptionalTime"/>
            <xs:element minOccurs="0" name="operator" type="xs:string"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="OfferBarAction">
        <xs:complexContent>
            <xs:extension base="tl:OfferAction">
                <xs:sequence>
                    <xs:element name="barLevel" type="xs:int"/>
                    <xs:element name="reason" type="xs:string"/>
                </xs:sequence>
                <xs:attribute default="false" name="isCollections" type="xs:boolean"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="OfferTransferAction">
        <xs:complexContent>
            <xs:extension base="tl:OfferAction">
                <xs:sequence>
                    <xs:element name="targetCustomerId" type="xs:long"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType abstract="true" name="OfferChangeAction">
        <xs:complexContent>
            <xs:extension base="tl:OfferAction"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="OfferUpgradeAction">
        <xs:complexContent>
            <xs:extension base="tl:OfferChangeAction">
                <xs:sequence>
                    <xs:group ref="tl:upgradeInformation"/>
                    <xs:choice maxOccurs="1" minOccurs="0">
                        <xs:element name="baseOffer" type="tl:ChangeCustomizedBaseOffer"/>
                        <xs:element name="bundleOffer" type="tl:ChangeCustomizedBundleOffer"/>
                    </xs:choice>
                    <xs:choice>
                        <xs:element name="channelId" type="xs:long"/>
                        <xs:element name="channelKey" type="xs:string"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="OfferModifyAction">
        <xs:complexContent>
            <xs:extension base="tl:OfferChangeAction">
                <xs:sequence>
                    <xs:choice maxOccurs="1" minOccurs="0">
                        <xs:element name="baseOffer" type="tl:ChangeCustomizedBaseOffer"/>
                        <xs:element name="bundleOffer" type="tl:ChangeCustomizedBundleOffer"/>
                    </xs:choice>
                    <xs:choice maxOccurs="1" minOccurs="0">
                        <xs:element name="channelId" type="xs:long"/>
                        <xs:element name="channelKey" type="xs:string"/>
                    </xs:choice>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="offerProposals" type="tl:OfferProposals"/>
    <xs:complexType name="OfferProposals">
        <xs:annotation>
            <xs:documentation>
                Contains a list of proposals for requested offers.
                OfferProposals can either be a base or
                bundle offer. Bundle
                offer
                include information about selected and available base offers and
                offer features.
                This includes
                selected and available
                service and offer
                features including parameters, modification
                information
                and charges.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="offerProposal" type="tl:AbstractOfferProposal"/>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="abstractOfferProposal" type="tl:AbstractOfferProposal"/>
    <xs:complexType abstract="true" name="AbstractOfferProposal">
        <xs:annotation>
            <xs:documentation>
                An OfferProposals can either be a base or bundle
                offer.
                Bundle offer includes information
                about selected and
                available
                base offers and bundle offer features.
                This includes selected and
                available
                service and offer
                features
                including
                parameters,
                modification information and charges.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="offerKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The unique key of the offer used to identify it.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="internalName" type="xs:string"/>
            <xs:element maxOccurs="1" minOccurs="0" name="invoiceName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The translated invoice text as defined in the
                        product configuration.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="description" type="xs:string"/>
            <xs:element minOccurs="0" name="customizedOfferId" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The id of the changed / updated product. Not
                        filled for new offers.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="validationErrors" type="tl:ValidationErrors">
                <xs:annotation>
                    <xs:documentation>
                        A list of error messages pertaining to this proposal.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="modification" type="tl:ProductCatalogOfferModification"/>
            <xs:element minOccurs="0" name="featureGroups" type="tl:FeatureGroupProposals">
                <xs:annotation>
                    <xs:documentation>
                        The possible and selected offer feature groups of this single offer.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="featureGroupCategories" type="tl:FeatureGroupCategories">
                <xs:annotation>
                    <xs:documentation>
                        The possible feature group categories of this single offer.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ProductCatalogOfferModification">
        <xs:annotation>
            <xs:documentation>
                Information about the existing offer this proposal has been created from as well as additional
                properties controlling the behaviour of upgrades from this existing offer to the offer defined in the
                proposal.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="sourceCustomizedOfferId" type="xs:long">
                <xs:annotation>
                    <xs:documentation>
                        The id of the changed / updated product. Not
                        filled for new offers.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="type" type="xs:string">
                <xs:annotation>
                    <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                        Type of the offer modification:
                        <dl>
                            <dt>NEW</dt>
                            <dd>offer is to be newly created</dd>
                            <dt>CHANGE</dt>
                            <dd>offer is to be upgraded from an existing offer</dd>
                            <dt>MODIFY</dt>
                            <dd>existing offer is modified</dd>
                            <dt>DELETE</dt>
                            <dd>existing offer is deleted</dd>
                            <dt>IGNORE</dt>
                            <dd>offer is ignored for display purposes</dd>
                        </dl>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="sourceOfferKey" type="xs:string"/>
            <xs:element minOccurs="0" name="sourceOfferInvoiceName" type="xs:string"/>
            <xs:element minOccurs="0" name="sourceParentOfferId" type="xs:long">
                <xs:annotation>
                    <xs:documentation>
                        The bundle offer id, if the selected offer is a
                        bundle member.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="upgrade" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if the current proposal is an upgrade from this existing offer, false if it is any other
                        type of operation.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="ruleKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        In the case of an upgrade, which rule is to be used to upgrade the existing offer to the offer
                        defined in the proposal.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="etfHandling" type="xs:string">
                <xs:annotation>
                    <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                        In the case of an upgrade, which early termination fee handling should be used. Possible types:
                        <dl>
                            <dt>CALCULATED</dt>
                            <dd>
                                a caluclated amount depending on how long the components to be terminated have been
                                active
                            </dd>
                            <dt>FIXED</dt>
                            <dd>a fixed amount defined in etfAmount</dd>
                            <dt>TOTAL</dt>
                            <dd>
                                the total initial cost of the components to be terminated
                            </dd>
                            <dt>WAIVED</dt>
                            <dd>no early termination fee</dd>
                        </dl>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="etfAmount" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The amount of the early terminaton fee for a etfHandling value of FIXED.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="upDowngradeFee" type="tl:SelectableFeature">
                <xs:annotation>
                    <xs:documentation>
                        A fee to be added to the proposed offer in case of an upgrade.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="validationError" type="tl:ValidationError"/>
    <xs:complexType name="ValidationError">
        <xs:sequence>
            <xs:element minOccurs="0" name="key" type="xs:string"/>
            <xs:element minOccurs="0" name="message" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A translated human readable error message.
                        Including place holders
                        for the given
                        arguments
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="arguments" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A list of arguments to be filled in the place
                        holders of the error
                        message.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="bundleOfferProposal" type="tl:BundleOfferProposal"/>
    <xs:complexType name="BundleOfferProposal">
        <xs:annotation>
            <xs:documentation>
                A Bundle offer proposal includes information about
                selected,
                terminated and available base
                offers and offer
                features
                including parameters, modification information and charges.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:AbstractOfferProposal">
                <xs:sequence>
                    <xs:element name="offerGroups" type="tl:OfferGroupProposals">
                        <xs:annotation>
                            <xs:documentation>
                                Contains a list of offer groups including
                                information about selectable
                                base
                                offers and
                                selected base offer
                                instances.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="unbounded" minOccurs="0" name="terminatedBaseOfferProposals"
                                type="tl:TerminatedBaseOfferProposal">
                        <xs:annotation>
                            <xs:documentation>
                                Contains base offers to be deleted in case of a
                                bundle modifiy or
                                up/downgrade.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:element name="baseOfferProposals" type="tl:BaseOfferProposals"/>
    <xs:complexType name="BaseOfferProposals">
        <xs:annotation>
            <xs:documentation>
                Contains a list of selected base offers with
                selected and available offer
                features, services
                and service
                features
                including parameters, modification information and charges.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="1" name="baseOffer" type="tl:BaseOfferProposal"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="BaseOfferProposal">
        <xs:annotation>
            <xs:documentation>
                A base offer proposal is a selected and available
                offer features,
                services and service
                features
                including
                parameters,
                modification information and charges.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:AbstractOfferProposal">
                <xs:sequence>
                    <xs:element minOccurs="0" name="serviceProposals" type="tl:ServiceProposals">
                        <xs:annotation>
                            <xs:documentation>
                                The possible and selected services of this single offer.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="TerminatedBaseOfferProposal">
        <xs:annotation>
            <xs:documentation>
                A base offer that is to be terminated.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:BaseOfferProposal">
                <xs:sequence/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="SelectableFeatures">
        <xs:annotation>
            <xs:documentation>
                A list of selectable features for a given component (offer or service).
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="selectableFeature" type="tl:SelectableFeature"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SelectableFeature">
        <xs:annotation>
            <xs:documentation>
                One selectable feature. This is just a template for possible values which will when selected
                generate feature instances.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:AbstractFeatureProposal">
                <xs:sequence>
                    <xs:element name="selectable" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>
                                Defines if this feature is still selectable.
                                As soon as a defined maximal number of instances of a feature is reached, this changes
                                from true to false.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ServiceFeatureProposal">
        <xs:annotation>
            <xs:documentation>
                A service feature proposal. Can have parameters.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="parameters" type="tl:ParameterProposals"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ParameterProposals">
        <xs:annotation>
            <xs:documentation>
                A list of feature parameter proposals.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="parameter" type="tl:ParameterProposal"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ParameterProposal">
        <xs:annotation>
            <xs:documentation>
                A proposal for a parameter of a feature
                containing the definition of a value
                that can be filled for the instance
                created from the parameter proposal's feature.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="defaultValue" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The parameter's default value on instantiation if no other value is set
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="type" type="xs:string">
                <xs:annotation>
                    <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                        The parameter's type. There are the following standard types:
                        <dl>
                            <dt>ADDRESS</dt>
                            <dd>
                                an address. The value must be JSON encoded to be correctly parsed by the system.
                            </dd>
                            <dt>BOOLEAN</dt>
                            <dd>true or false</dd>
                            <dt>DATE</dt>
                            <dd>a date</dd>
                            <dt>LIST</dt>
                            <dd>a list of values separated by ','</dd>
                            <dt>NONE</dt>
                            <dd>no parameter</dd>
                            <dt>NUMBER</dt>
                            <dd>a numeric value</dd>
                            <dt>PHONE_NUMBER</dt>
                            <dd>a phone number</dd>
                            <dt>SELECT</dt>
                            <dd>
                                a list of possible values, defined in selectableValues.
                            </dd>
                            <dt>STRING</dt>
                            <dd>a free form string</dd>
                        </dl>
                        Further types may be defined in supplier management.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="regexValidationRule" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A regular expression this value must conform to.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A textual description of the value.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="configurable" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Currently unused.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="modified" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Currently unused.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="readOnly" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        The parameter can't be changed from the outside.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="changeable" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        The parameter can be changed from the outside.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="validationErrors" type="tl:ValidationErrors">
                <xs:annotation>
                    <xs:documentation>
                        Filled if the parameter's value could not be validated (either it was empty or didn't conform to
                        validation rules).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="selectableValues" type="tl:SelectableValues">
                <xs:annotation>
                    <xs:documentation>
                        A list of values one of which can be selected. Used for parameter values of type SELECT.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SelectableValues">
        <xs:annotation>
            <xs:documentation>
                One selectable value for a parameter value of type SELECT.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="key" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The key of this value. Used to identify the value uniquely.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="displayName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The display name of this value for display to the end user.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Textual description of the value.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="displayOrder" type="xs:int">
                <xs:annotation>
                    <xs:documentation>
                        The order in which the values are to be presented to the end user.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="selected" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        true if the value is the one selected for this parameter.
                        Can be true only for one value of the list of values.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="charge" type="tl:AbstractCharge">
                <xs:annotation>
                    <xs:documentation>
                        The charge associated with selecting that value. Different
                        values may have different charges associated with them.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType abstract="true" name="AbstractCharge">
        <xs:annotation>
            <xs:documentation>
                Common attributes of a charge.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="chargeType" type="xs:string">
                <xs:annotation>
                    <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                        The type of the charge. May be one of the following:
                        <dl>
                            <dt>ONCE_ONLY</dt>
                            <dd>to be charged once.</dd>
                            <dt>RECURRING</dt>
                            <dd>
                                to be charged in a defined frequency until a defined end date.
                            </dd>
                            <dt>INSTALMENT</dt>
                            <dd>
                                to be charged with a defined number of instalments.
                            </dd>
                            <dt>DISCOUNT</dt>
                            <dd>
                                discounting other charges of the offer or account.
                            </dd>
                        </dl>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="invoiceText" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The text appearing on the invoice if this charge is instantiated for the customer.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="glCode" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A code identifying the charge for general ledger reporting.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="productCode" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A code identifying the charge internally.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="billTypeKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The bill type this charge is associated with.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="taxRate" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The charge's tax rate.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="netCharge" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The charge's net value.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="grossCharge" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The charge's gross value.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="chargeCurrency" type="tl:isoCurrency">
                <xs:annotation>
                    <xs:documentation>
                        The charge's currency.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="RecurringChargeProposal">
        <xs:annotation>
            <xs:documentation>
                Attributes of a recurring charge.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:AbstractCharge">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="1" name="paymentFrequencyMonths" type="xs:long">
                        <xs:annotation>
                            <xs:documentation>
                                The payment frequency of this charge in months. 1 means it's charged monthly, 2
                                bimonthly and so on.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="1" name="chargeInAdvanceMonths" type="xs:long">
                        <xs:annotation>
                            <xs:documentation>
                                How many months in advance this charge is charged.
                                If set to 1 the charge for February is charged in January and so on.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="OnceOnlyChargeProposal">
        <xs:annotation>
            <xs:documentation>
                Attributes of a once only charge.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:AbstractCharge"/>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="InstalmentChargeProposal">
        <xs:annotation>
            <xs:documentation>
                Attributes of an instalment charge.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:AbstractCharge">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="1" name="frequencyMonths" type="xs:long">
                        <xs:annotation>
                            <xs:documentation>
                                The payment frequency of this charge in months. 1 means it's charged monthly, 2
                                bimonthly and so on.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="payRemainderOnTermination" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>
                                Should the remaining instalments be paid when the charge is terminated.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="1" name="initialPayments" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>
                                Number of initial payments on the first charging of this instalment.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="1" name="numberOfPayments" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>
                                Number of payments on this instalment.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="numberOfPaymentsToMake" type="xs:int">
                        <xs:annotation>
                            <xs:documentation>
                                Number of yet open payments on this instalment.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="OfferGroupProposal">
        <xs:annotation>
            <xs:documentation>
                Contains a list of offer groups including
                information about selectable base
                offers and
                selected base offer
                instances.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="key" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The key of the offer group.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="description" type="xs:string"/>
            <xs:element maxOccurs="1" minOccurs="0" name="numberOfSelectedInstances" type="xs:int">
                <xs:annotation>
                    <xs:documentation>
                        How many members of this group are currently selected.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="minNumberOfInstances" type="xs:short">
                <xs:annotation>
                    <xs:documentation>
                        How many members of this group have to be selected minimally.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="maxNumberOfInstances" type="xs:short">
                <xs:annotation>
                    <xs:documentation>
                        How many members of this group can maximally be selected.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="baseOffers" type="tl:BaseOfferProposals">
                <xs:annotation>
                    <xs:documentation>
                        Contains the list of selected base offer
                        proposals.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="selectableGroupOffers" type="tl:SelectableGroupOffers">
                <xs:annotation>
                    <xs:documentation>
                        Contains the list of selectable base offers.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="OfferGroupProposals">
        <xs:annotation>
            <xs:documentation>
                Contains a list of offer groups including
                information about selectable base
                offers and
                selected base offer
                instances.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="offerGroup" type="tl:OfferGroupProposal"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SelectableGroupOffers">
        <xs:annotation>
            <xs:documentation>
                Contains the list of selectable base offer.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="selectableGroupOffer" type="tl:SelectableGroupOffer"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SelectableGroupOffer">
        <xs:sequence>
            <xs:element name="offerKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The key identifying this offer.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Textual description of this offer.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="invoiceName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Text appearing on the invoice for this offer.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="internalName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Internal name for this offer.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element default="true" name="selectable" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Specifies if this base offer is currently
                        selectable in the containing
                        group.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AbstractFeatureGroupProposal">
        <xs:annotation>
            <xs:documentation>
                Common attributes of a feature group proposal. A feature group proposal is a container for
                feature proposals that can be selected for the component the group resides in.
                There are feature groups for offers and
                feature groups for services.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="displayName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        For displaying this feature group to the end user.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="featureGroupType" type="xs:string">
                <xs:annotation>
                    <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                        The type of features this feature group proposal contains. Can be one of the following:
                        <dl>
                            <dt>EIP</dt>
                            <dd>
                                equipment instalment plans (equipments with an instalment charge).
                            </dd>
                            <dt>EQUIPMENT_RENTAL</dt>
                            <dd>equipments with a recurring charge.</dd>
                            <dt>EQUIPMENT</dt>
                            <dd>equipments with a once only charge.</dd>
                            <dt>LEVEL_COMMAND</dt>
                            <dd>allowances</dd>
                            <dt>DISCOUNT</dt>
                            <dd>discounts</dd>
                            <dt>CONTRACT</dt>
                            <dd>contracts</dd>
                            <dt>DEPOSIT</dt>
                            <dd>deposits</dd>
                            <dt>NONE</dt>
                            <dd>
                                simple charges (recurring, once only or instalment)
                            </dd>
                        </dl>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="numberOfAllowedInstances" type="xs:int">
                <xs:annotation>
                    <xs:documentation>
                        Number of feature instances this feature group may have.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="featureSelectionMandatory" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if a feature from this group has to be selected.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="single" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if a there can be only one feature in this group.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="selectable" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if there can yet be features selected in this group.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="modified" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if this group has been modified internally in the last roundtrip (i.e. a feature dependency
                        selected a feature in this group automatically).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SelectableFeatureGroupProposals">
        <xs:annotation>
            <xs:documentation>
                Container for selectable feature group proposal. Contains attributes of an abstract feature group as
                well as a list of selectable features.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:AbstractFeatureGroupProposal">
                <xs:sequence>
                    <xs:element minOccurs="0" name="selectableFeatures" type="tl:SelectableFeatures"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ServiceFeatureGroupProposal">
        <xs:annotation>
            <xs:documentation>
                Container for a service feature group proposal. Contains attributes of an abstract feature group as well
                as a list of service feature propsals.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:FeatureGroupProposal">
                <xs:sequence>
                    <xs:element minOccurs="0" name="serviceFeatureProposal" type="tl:ServiceFeatureProposal"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="FeatureGroupProposals">
        <xs:annotation>
            <xs:documentation>
                Container for a a list of feature group proposals.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="featureGroup" type="tl:FeatureGroupProposal"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="FeatureGroupProposal">
        <xs:annotation>
            <xs:documentation>
                An actual feature group proposal containing feature instances.
                Created from a selectable feature group proposal when one of the containing features is selected.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="key" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The feature group's key.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="displayName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Display name of this group to be shown to the end user.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Description of the group.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="featureGroupType" type="xs:string">
                <xs:annotation>
                    <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                        The type of features this feature group proposal contains. Can be one of the following:
                        <dl>
                            <dt>EIP</dt>
                            <dd>
                                equipment instalment plans (equipments with an instalment charge).
                            </dd>
                            <dt>EQUIPMENT_RENTAL</dt>
                            <dd>equipments with a recurring charge.</dd>
                            <dt>EQUIPMENT</dt>
                            <dd>equipments with a once only charge.</dd>
                            <dt>LEVEL_COMMAND</dt>
                            <dd>allowances</dd>
                            <dt>DISCOUNT</dt>
                            <dd>discounts</dd>
                            <dt>CONTRACT</dt>
                            <dd>contracts</dd>
                            <dt>DEPOSIT</dt>
                            <dd>deposits</dd>
                            <dt>NONE</dt>
                            <dd>
                                simple charges (recurring, once only or instalment)
                            </dd>
                        </dl>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="featureSelectionMandatory" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if a feature from this group has to be selected.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="numberOfAllowedInstances" type="xs:int">
                <xs:annotation>
                    <xs:documentation>
                        Number of feature instances this feature group may have.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="numberOfSelectedInstances" type="xs:int">
                <xs:annotation>
                    <xs:documentation>
                        Number of feature instances this feature group currently has selected.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="minNumberOfInstances" type="xs:short">
                <xs:annotation>
                    <xs:documentation>
                        Minimal number of instances this feature group has to have.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="maxNumberOfInstances" type="xs:short">
                <xs:annotation>
                    <xs:documentation>
                        Number of feature instances this feature group may have.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="single" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if a there can be only one feature in this group.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="modified" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if this group has been modified internally in the last roundtrip (i.e. a feature dependency
                        selected a feature in this group automatically).
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="selectable" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if there can yet be features selected in this group.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="categoryKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The category key of this feature group
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="validationErrors" type="tl:ValidationErrors">
                <xs:annotation>
                    <xs:documentation>
                        Contains values if this feature group is not correctly configured.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="featureInstances" type="tl:FeatureInstances">
                <xs:annotation>
                    <xs:documentation>
                        The selected feature instances of this group.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="selectableFeatures" type="tl:SelectableFeatures">
                <xs:annotation>
                    <xs:documentation>
                        The features that could be selected for this group.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="FeatureInstances">
        <xs:annotation>
            <xs:documentation>
                A container for selected instances of a feature.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="featureInstance" type="tl:FeatureInstance"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AbstractFeatureProposal">
        <xs:annotation>
            <xs:documentation>
                Common attributes for features.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Textual description of the feature
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="displayName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Name to be displayed to the end user.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="provisionable" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Describes if the lifecycle of the feature is bound to the offer (false), or if this
                        feature's lifecycle is controlled by provisioning, i.e. the activation and deactivation
                        is controlled from the outside.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="parentChargeClusterKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The feature may have an overrider. If yes, this is the charge cluster identifier of that
                        overrider.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="charge" type="tl:AbstractCharge">
                <xs:annotation>
                    <xs:documentation>
                        The charge of the feature.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="externalReferenceKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A link to the inventory system for equipment type features.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="hidden" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Tells if the feature should be displayed to the end user.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="changeable" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Tells if the feature's parameters are fixed.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="name" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The name of the feature.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="displayOrder" type="xs:int">
                <xs:annotation>
                    <xs:documentation>
                        Order of this feature inside its group.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="parameters" type="tl:ProductCatalogFeatureParameters">
                <xs:annotation>
                    <xs:documentation>
                        List of parameters asssociated with this feature. Only used for service features.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="featureInstance" type="tl:FeatureInstance"/>
    <xs:complexType name="FeatureInstance">
        <xs:annotation>
            <xs:documentation>
                A selected instance of a feature.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:AbstractFeatureProposal">
                <xs:sequence>
                    <xs:element name="deletable" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>
                                Can this instance be deleted.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="modified" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>
                                Has this instance been modified by the system (i.e. been activated by a feature
                                dependency).
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="validationErrors" type="tl:ValidationErrors">
                        <xs:annotation>
                            <xs:documentation>
                                Filled if there have been errors for this feature.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="serviceProposal" type="tl:ServiceProposal">
                        <xs:annotation>
                            <xs:documentation>
                                Equipment features may have a service associated with them.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="modification"
                                type="tl:ProductCatalogFeatureModification">
                        <xs:annotation>
                            <xs:documentation>
                                If the feature is based on an existing feature, this contains the information about the
                                crm instance the feature is based on.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="ProductCatalogFeatureModification">
        <xs:annotation>
            <xs:documentation>
                For feature instances based on existing features contains information about the crm instance the feature
                is based on
                as well as possible termination charges incurred when the feature is removed.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="accountComponentId" type="xs:long">
                <xs:annotation>
                    <xs:documentation>
                        The crm identifier for the account component the feature instance is based on.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="grossCharge" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The gross charge that has been instantiated in crm for this feature.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="netCharge" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The net charge that has been instantiated in crm for this feature.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="chargeCurrency" type="tl:isoCurrency">
                <xs:annotation>
                    <xs:documentation>
                        The currency of this charge that has been instantiated in crm for this feature.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="taxRate" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The tax rate used for the feature.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="activationDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>
                        When the feature was activated in crm.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="displayName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Name to be shown to the end user.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="provisionable" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Describes if the lifecycle of the feature is bound to the offer (false), or if this
                        feature's lifecycle is controlled by provisioning, i.e. the activation and deactivation
                        is controlled from the outside.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="openPayments" type="xs:int">
                <xs:annotation>
                    <xs:documentation>
                        If the feature has an instalment charge, how many payments are still open.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="numberOfPayments" type="xs:int">
                <xs:annotation>
                    <xs:documentation>
                        If the feature has an instalment charge, how many payments were there.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="payRemainderOnTermination" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        If the feature has an instalment charge, are the remaining charges to be paid on termination.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="serviceId" type="xs:long">
                <xs:annotation>
                    <xs:documentation>
                        The crm identifier of the service the feature has attached to it.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="type" type="xs:string">
                <xs:annotation>
                    <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                        The modification type of the feature. It may be:
                        <dl>
                            <dt>CREATE</dt>
                            <dd>the feature is to be newly created</dd>
                            <dt>UPDATE</dt>
                            <dd>
                                the feature is to be upgraded from an existing feature
                            </dd>
                            <dt>RETAIN</dt>
                            <dd>
                                the feature should be retained on upgrade to another offer
                            </dd>
                            <dt>DELETE</dt>
                            <dd>the feature is to be deleted</dd>
                            <dt>IGNORE</dt>
                            <dd>the feature is to be ignored</dd>
                        </dl>
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="featureType" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Type of the feature. Corresponds with the feature group type.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="terminationCharge" type="tl:PricedChargeCluster">
                <xs:annotation>
                    <xs:documentation>
                        The termination charge to be calculated when this feature is DELETEd.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="pricedChargeCluster" type="tl:PricedChargeCluster"/>
    <xs:complexType name="PricedChargeCluster">
        <xs:annotation>
            <xs:documentation>
                Information about termination charges that would have to be paid if the feature containing this element
                was to be removed.
                Used for contracts.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="chargeClusterKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The charge cluster of the termination charge.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="netChargeAmount" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The net charge of the termination charge.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="grossChargeAmount" type="xs:decimal">
                <xs:annotation>
                    <xs:documentation>
                        The gross charge of the termination charge.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="chargeCurrency" type="tl:isoCurrency">
                <xs:annotation>
                    <xs:documentation>
                        The charge currency of the termination charge.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="graceDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>
                        The date from which a configurable grace period starts in which the termination charge is
                        waived.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="plannedEndDate" type="xs:dateTime">
                <xs:annotation>
                    <xs:documentation>
                        The planned end date for this termination charge, after which the termination charge would no
                        longer be considered.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="ModificationType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="CREATE"/>
            <xs:enumeration value="UPDATE"/>
            <xs:enumeration value="DELETE"/>
            <xs:enumeration value="RETAIN"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="FeatureType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="MANDATORY"/>
            <xs:enumeration value="PROVISIONED"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="ProductCatalogFeatureParameters">
        <xs:annotation>
            <xs:documentation>
                Container for feature parameter instances.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="parameter" type="tl:ProductCatalogFeatureParameter"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ProductCatalogFeatureParameter">
        <xs:annotation>
            <xs:documentation>
                A feature parameter instance.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="value" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The parameter's value.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="defaultValue" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The parameter's default value.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        Textual descriptio of the parameter.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="type" type="xs:string">
                <xs:annotation>
                    <xs:documentation xmlns="http://www.w3.org/1999/xhtml">
                        The parameter's type. There are the following standard types:
                        <dl>
                            <dt>ADDRESS</dt>
                            <dd>
                                an address. The value must be JSON encoded to be correctly parsed by the system.
                            </dd>
                            <dt>BOOLEAN</dt>
                            <dd>true or false</dd>
                            <dt>DATE</dt>
                            <dd>a date</dd>
                            <dt>LIST</dt>
                            <dd>a list of values separated by ','</dd>
                            <dt>NONE</dt>
                            <dd>no parameter</dd>
                            <dt>NUMBER</dt>
                            <dd>a numeric value</dd>
                            <dt>PHONE_NUMBER</dt>
                            <dd>a phone number</dd>
                            <dt>SELECT</dt>
                            <dd>
                                a list of possible values, defined in selectableValues.
                            </dd>
                            <dt>STRING</dt>
                            <dd>a free form string</dd>
                        </dl>
                        Further types may be defined in supplier management.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="configurable" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Currently unused.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="modified" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        Currently unused.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="readOnly" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        The parameter's value can't be changed from the outside.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="changeable" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        The parameter can be changed from the outside.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="validationRule" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A regular expression this value must conform to or a service locator lookup for a validation
                        bean.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="selected" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if the parameter is selected.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="validationErrors" type="tl:ValidationErrors">
                <xs:annotation>
                    <xs:documentation>
                        Filled if the parameter's value is not filled and required or does not conform to validation
                        rules.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="validationRuleDefaultErrorMessage" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        A default error message that will be added to the validation errors if the validationRule does
                        not match.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="parameterValues" type="tl:ProductCatalogFeatureParameterValues">
                <xs:annotation>
                    <xs:documentation>
                        A list of values one of which can be selected. Used for parameter values of type SELECT.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="validationRuleType" type="tl:ValidationRuleType">
                <xs:annotation>
                    <xs:documentation>
                        Type of validation rule. Can be REGEX or BEAN.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="modification"
                        type="tl:ProductCatalogFeatureParameterModification">
                <xs:annotation>
                    <xs:documentation>
                        Information about an existing crm value for this parameter.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:element name="ValidationErrors" type="tl:ValidationErrors"/>
    <xs:complexType name="ValidationErrors">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="validationError" type="tl:ValidationError"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ProductCatalogFeatureParameterModification">
        <xs:annotation>
            <xs:documentation>
                Information about an existing crm value for a feature parameter.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="value" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The parameter's value as stored in crm.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The parameter's description.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:simpleType name="ValidationRuleType">
        <xs:restriction base="xs:string">
            <xs:enumeration value="BEAN"/>
            <xs:enumeration value="REGEX"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:complexType name="ProductCatalogFeatureParameterValues">
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="parameterValue"
                        type="tl:ProductCatalogFeatureParameterValue"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ProductCatalogFeatureParameterValue">
        <xs:annotation>
            <xs:documentation>
                One selectable value of a parameter of type SELECT.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="1" minOccurs="0" name="key" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The value's key.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="displayName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The value's name to be displayed to the end user.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The value's description.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="parentChargeClusterKey" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The value may have an overrider. If yes, this is the charge cluster identifier of that
                        overrider.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="displayOrder" type="xs:int">
                <xs:annotation>
                    <xs:documentation>
                        Order of the value in its parameter.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element name="selected" type="xs:boolean">
                <xs:annotation>
                    <xs:documentation>
                        True if the value has been selected. Only one value may be selected per parameter at a time.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="charge" type="tl:AbstractCharge">
                <xs:annotation>
                    <xs:documentation>
                        The charge associated with the value.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ServiceProposals">
        <xs:annotation>
            <xs:documentation>
                Container for service proposals.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="serviceProposal" type="tl:ServiceProposal"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="ServiceProposal">
        <xs:annotation>
            <xs:documentation>
                A proposal for a service to be created.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element minOccurs="0" name="key" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The key identifiying the service.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="accountComponentId" type="xs:long">
                <xs:annotation>
                    <xs:documentation>
                        If the service proposal is based on an existing service, this is the crm identifier of that
                        service.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="displayName" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The service's display name
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="description" type="xs:string">
                <xs:annotation>
                    <xs:documentation>
                        The service's description
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="displayOrder" type="xs:int">
                <xs:annotation>
                    <xs:documentation>
                        The service's display order.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element minOccurs="0" name="featureGroups" type="tl:FeatureGroupProposals">
                <xs:annotation>
                    <xs:documentation>
                        Feature groups associated with this service.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="charges" type="tl:AbstractCharges">
                <xs:annotation>
                    <xs:documentation>
                        A service may have mulitple charges, especially usage charges associated with it.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="serviceProposals" type="tl:ServiceProposals">
                <xs:annotation>
                    <xs:documentation>
                        Sub services of this service.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
            <xs:element maxOccurs="1" minOccurs="0" name="selectableServices" type="tl:SelectableServices">
                <xs:annotation>
                    <xs:documentation>
                        List of selectable sub services.
                    </xs:documentation>
                </xs:annotation>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SelectableServices">
        <xs:annotation>
            <xs:documentation>
                A list of selectable services.
            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="selectableService" type="tl:SelectableService"/>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="SelectableService">
        <xs:annotation>
            <xs:documentation>
                One selectable service. This is just a
                template for possible values which will when selected
                generate service instances.
            </xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="tl:ServiceProposal">
                <xs:sequence>
                    <xs:element maxOccurs="1" minOccurs="0" name="minNumberOfInstances" type="xs:short">
                        <xs:annotation>
                            <xs:documentation>
                                How many instances of this service have to be selected minimally.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element maxOccurs="1" minOccurs="0" name="maxNumberOfInstances" type="xs:short">
                        <xs:annotation>
                            <xs:documentation>
                                How many instances of this service can maximally be selected.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                    <xs:element name="selectable" type="xs:boolean">
                        <xs:annotation>
                            <xs:documentation>
                                Defines if this feature is still selectable.
                                As soon as a defined maximal number of instances of a feature is
                                reached, this changes
                                from true to false.
                            </xs:documentation>
                        </xs:annotation>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="AbstractCharges">
        <xs:sequence>
